{
    "name": "innomedio/sulu-image-optimizer-bundle",
    "type": "symfony-bundle",
    "description": "Optimizes images before they are being uploaded and stored in the Sulu media library",
    "keywords": [
        "innomedio",
        "symfony",
        "sulu",
        "image",
        "upload"
    ],
    "homepage": "https://www.innomedio.be",
    "license": "MIT",
    "authors": [
        {
            "name": "Ewald Vanderveken",
            "email": "ewald.vanderveken@innomedio.be"
        }
    ],
    "require": {
        "php": "^8.2",
        "sulu/sulu": "^3.0",
        "spatie/image": "^3.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpro/grumphp": "^2.4",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Innomedio\\Sulu\\ImageOptimizerBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpro/grumphp": true,
            "php-http/discovery": true
        }
    }
}
